style="border:2px solid #d3d3d3;">
</canvas>

</div>

<img src="Salao.png" id="image" alt="Supermercado" width="0" height="0">



<script type="text/javascript">
	var x = <%=data.posx%>;
	var y = <%=data.posy%>;
	if( x != -1){
		PrintPositions(x,y);
		refresh();
	}else{
		PrintNoDots();
		refresh();
	}
		

</script>


